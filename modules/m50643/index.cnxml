  <m:msub>
  <m:mi> a </m:mi>
  <m:mi> x </m:mi>
  </m:msub>
  <m:msub>
  <m:mi> b </m:mi>
  <m:mi> z </m:mi>
  </m:msub>
  <m:mi mathvariant="bold"> j </m:mi>
