  <m:mi mathvariant="bold"> b </m:mi>

  <m:mo> = </m:mo>
  <m:mo> ( </m:mo>

  <m:msub>
  <m:mi> a </m:mi>
  <m:mi> x </m:mi>
  </m:msub>
  <m:mi mathvariant="bold"> i </m:mi>
  <m:mo> + </m:mo>
