  <m:mi> z </m:mi>
  </m:msub>
  <m:mi mathvariant="bold"> k </m:mi>
  <m:mo> ) </m:mo>

     </m:mtd>
   </m:mtr>



   <m:mtr>
     <m:mtd>
  <m:mo> ⇒ </m:mo>
  <m:mi mathvariant="bold"> a </m:mi>
  <m:mo> × </m:mo>
  <m:mi mathvariant="bold"> b </m:mi>

  <m:mo> = </m:mo>
  <m:msub>
  <m:mi> a </m:mi>
  <m:mi> x </m:mi>
  </m:msub>
 <m:mi mathvariant="bold"> i </m:mi>
  <m:mo> × </m:mo>

  <m:msub>
  <m:mi> b </m:mi>
  <m:mi> y </m:mi>
