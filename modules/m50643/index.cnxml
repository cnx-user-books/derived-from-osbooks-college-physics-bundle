  <m:msub>
  <m:mi> b </m:mi>
  <m:mi> z </m:mi>
  </m:msub>
  <m:mi mathvariant="bold"> k </m:mi>
  <m:mo> + </m:mo>

  <m:msub>
  <m:mi> a </m:mi>
  <m:mi> y </m:mi>
