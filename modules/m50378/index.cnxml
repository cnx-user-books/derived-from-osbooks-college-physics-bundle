						<m:mtd columnalign="left">
							<m:mrow>
								<m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo>×</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mn>3</m:mn>
								</m:msup>
								<m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mo>×</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mn>8</m:mn>
								</m:msup>
								<m:mo stretchy="false">)</m:mo>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mo>=</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo>×</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mn>3</m:mn>
								</m:msup>
								<m:mo>×</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mn>8</m:mn>
								</m:msup>
								<m:mo stretchy="false">)</m:mo>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mo>=</m:mo><m:mn>8</m:mn><m:mo>×</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mn>3</m:mn><m:mo>+</m:mo><m:mn>8</m:mn>
									</m:mrow>
								</m:msup>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mo>=</m:mo><m:mn>8</m:mn><m:mo>×</m:mo><m:msup>
									<m:mrow>
										<m:mn>10</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mn>11</m:mn>
									</m:mrow>
								</m:msup>
							</m:mrow>
						</m:mtd>
