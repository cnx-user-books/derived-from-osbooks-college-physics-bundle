 </m:semantics>
</m:math>

  </para>
	</solution>
</exercise><exercise id="eip-449"><problem id="eip-573">
		<para id="eip-753">
179,000,000,000,000,000,000 
  </para>
	</problem>
	<solution id="eip-405">
		<para id="eip-946"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>1.79</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mn>20</m:mn>
    </m:mrow>
   </m:msup>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
</para>
	</solution>
</exercise><exercise id="eip-739"><problem id="eip-294">
		<para id="eip-177">
100,000 
  </para>
	</problem>
	<solution id="eip-987">
		<para id="eip-420"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>1.0</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mn>5</m:mn>
   </m:msup>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
</para>
	</solution>
</exercise><exercise id="eip-27"><problem id="eip-225">
		<para id="eip-833">
1,000,000 
  </para>
	</problem>
	<solution id="eip-189">
		<para id="eip-792"><m:math>
