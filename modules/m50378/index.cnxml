						<m:mn>27</m:mn>
					</m:mrow>
				</m:msup>
				<m:mo>=</m:mo><m:mtext>1,000,000,000,000,000,000,000,000,000</m:mtext>
			</m:mrow>
			<m:annotation encoding="MathType-MTEF">
			</m:annotation>
		</m:semantics>
	</m:math>
</para>
</example><example id="eip-697"><para id="eip-21">
<m:math>
 <m:semantics>
  <m:mrow>
