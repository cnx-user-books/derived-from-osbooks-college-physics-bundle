   </m:msup>
   <m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>8.1</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>22</m:mn>
    </m:mrow>
   </m:msup>
   <m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4.05</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>4</m:mn>
    </m:mrow>
   </m:msup>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
</para>
</example></section><section id="eip-323"><title>Practice Set C</title><para id="eip-88">Perform each multiplication. </para><exercise id="eip-16"><problem id="eip-296">
