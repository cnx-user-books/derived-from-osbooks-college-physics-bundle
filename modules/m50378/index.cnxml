  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
  </para>
	</problem>
	<solution id="eip-373">
		<para id="eip-261"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>7.233</m:mn><m:mo>×</m:mo><m:mn>10</m:mn>
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
</para>
	</solution>
</exercise><exercise id="eip-182"><problem id="eip-510">
		<para id="eip-552">
 <m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>5387.7965</m:mn>
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
  </para>
	</problem>
	<solution id="eip-950">
		<para id="eip-564"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>5.3877965</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mn>3</m:mn>
   </m:msup>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
 </m:annotation>
 </m:semantics>
</m:math>
</para>
	</solution>
</exercise><exercise id="eip-453"><problem id="eip-45">
		<para id="eip-137">
87,000,000</para>
	</problem>
	<solution id="eip-823">
		<para id="eip-729"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>8.7</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>10</m:mn>
    </m:mrow>
    <m:mn>7</m:mn>
   </m:msup>
   
  </m:mrow>
 <m:annotation encoding="MathType-MTEF">
