					<m:mrow>
						<m:mo>−</m:mo><m:mn>5</m:mn>
					</m:mrow>
				</m:msup>
				<m:mo>=</m:mo><m:mtext>0</m:mtext><m:mtext>.0000421</m:mtext>
			</m:mrow>
			<m:annotation encoding="MathType-MTEF">
			</m:annotation>
		</m:semantics>
	</m:math>
</para>
</example><example id="eip-750"><para id="eip-146"><m:math>
		<m:semantics>
