                <m:mtr>
<m:mtd>
               
</m:mtd>
<m:mtd>
                      <m:mo stretchy="false">=</m:mo>
</m:mtd>
<m:mtd>
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:mfenced open="(" close=")">
