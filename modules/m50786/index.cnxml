                  </m:mrow>
                </m:msub>
                <m:mrow>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:mo stretchy="false">+</m:mo>
                  <m:msub>
                    <m:mi>m</m:mi>
                    <m:mrow>
