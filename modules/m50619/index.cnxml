                    </m:mrow>
                  </m:mrow>
                </m:mrow>
             
            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{T rSub { size 8{1} } ="736"" N"} {}</m:annotation>
          </m:semantics>
        </m:math> 
      </para><para id="import-auto-id3176573">
        <m:math>
          <m:semantics>
            <m:mrow>
              
                <m:mrow>
                  <m:mrow>
                    <m:msub>
                      <m:mi>T</m:mi>
                      
                        <m:mrow>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      
                    </m:msub>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mtext>194 N</m:mtext>
                  </m:mrow>
                </m:mrow>
              
              <m:mrow/>
            </m:mrow>
