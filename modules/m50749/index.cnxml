           </m:mrow>
          </m:msub>
          <m:mo stretchy="false">
=
          </m:mo>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:msup>
              <m:mi>
v
              </m:mi>
              <m:mrow>
                <m:mrow>
                 <m:mrow>
                  <m:mn>
2
                  </m:mn>
                 </m:mrow>
                </m:mrow>
              </m:mrow>
             </m:msup>
            </m:mrow>
            <m:mrow>
             <m:mi>
r
             </m:mi>
            </m:mrow>
           </m:mfrac>
          </m:mrow>
         </m:mrow>
         <m:mi>
;
         </m:mi><m:mspace width="0.25em"/>
         <m:mrow>
          <m:msub>
