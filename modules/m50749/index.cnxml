                            <m:mn>2</m:mn>
                          </m:mrow>
                        
                      </m:msup>
                      <m:mi>r</m:mi>
                    </m:mfrac>
                  </m:mrow>
                  <m:mi/>
                  <m:mi>;</m:mi><m:mspace width="0.25em"/>
                  <m:mrow>
                    <m:msub>
                      <m:mi>a</m:mi>
                      
                        <m:mrow>
                          <m:mtext>c</m:mtext>
                        </m:mrow>
                      
                    </m:msub>
                    <m:mo stretchy="false">=</m:mo>
                    <m:msup>
                      <m:mi fontstyle="italic">rω</m:mi>
                      
                        <m:mrow>
                          <m:mn>2</m:mn>
