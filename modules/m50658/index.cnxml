                <m:mrow>
                  <m:msub>
                    <m:mi>F</m:mi>
                    
                      <m:mrow>
                        <m:mtext>B</m:mtext>
                      </m:mrow>
                    
                  </m:msub>
                  <m:mo stretchy="false">=</m:mo>
                  <m:mfrac>
                    <m:mrow>
                      <m:mfenced open="(" close=")">
                        <m:mrow>
                          <m:mn>0</m:mn>
                          <m:mtext>.</m:mtext>
                          <m:mtext>160</m:mtext><m:mspace width="0.25em"/>
                          <m:mtext> m</m:mtext>
                        </m:mrow>
                      </m:mfenced>
                      <m:mfenced open="(" close=")">
                        <m:mrow>
                          <m:mn>2</m:mn>
                          <m:mtext>.</m:mtext>
                          <m:mtext>50</m:mtext><m:mspace width="0.25em"/>
                          <m:mtext> kg</m:mtext>
                        </m:mrow>
                      </m:mfenced>
                      <m:mrow>
                        <m:mfenced open="(" close=")">
                          <m:mrow>
                            <m:mn>9</m:mn>
                            <m:mtext>.</m:mtext>
                            <m:mtext>80</m:mtext><m:mspace width="0.25em"/>
