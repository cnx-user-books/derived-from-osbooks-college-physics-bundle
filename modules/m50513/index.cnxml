  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>5</m:mn><m:mo>,</m:mo><m:mfrac>
     <m:mi>π</m:mi>
     <m:mn>2</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
</m:math>
</para>
</problem> 
<solution id="fs-id1165134081844">
 

<media id="fs-id1165134081850" alt="Polar coordinate system with a point located on the fifth concentric circle and pi/2.">
<image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_08_03_210.jpg"/>
</media>
</solution></exercise><exercise id="fs-id1165134167348">
<problem id="fs-id1165134167350">
<para id="fs-id1165134167352"><m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>4</m:mn><m:mo>,</m:mo><m:mfrac>
     <m:mrow>
      <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>π</m:mi></m:mrow>
     <m:mn>4</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
</m:math>
</para>
</problem>
</exercise> 
<exercise id="fs-id1165134394439"><problem id="fs-id1165135648722">
<para id="fs-id1165135430921"><m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>3</m:mn><m:mo>,</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>5</m:mn><m:mi>π</m:mi></m:mrow>
