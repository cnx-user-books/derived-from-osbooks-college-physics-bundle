                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ \( t rSub { size 8{1} }  \)  rSup { size 8{2} } ="16" "." "092"`s rSup { size 8{2} } } {}</m:annotation>
            </m:semantics>
