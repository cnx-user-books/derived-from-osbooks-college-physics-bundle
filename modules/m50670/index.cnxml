             <m:mrow>
              <m:mn>
2
              </m:mn>
             </m:mrow>
            </m:mrow>
          </m:mrow>
         </m:msup>
         <m:mo stretchy="false">
)
         </m:mo>
         <m:mtext>
s
         </m:mtext>
        </m:mrow>
       </m:mrow>
      <m:mrow/>
     </m:mrow>
     <m:annotation encoding="StarMath 5.0">
size 12{ \( "N/m" rSup { size 8{2} }  \) s} {}
     </m:annotation>
    </m:semantics>
   </m:math> or <m:math>
