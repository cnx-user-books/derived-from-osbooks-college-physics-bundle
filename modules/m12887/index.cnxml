                     <m:mo/>
                     <m:mi>k</m:mi>
                     <m:mo/>
                     <m:mi>x</m:mi>
                   </m:mrow>
                 </m:msup>
                 <m:mo/>
                 <m:mrow>
                   <m:mo form="prefix" largeop="true">∫</m:mo>
                   <m:mrow>
