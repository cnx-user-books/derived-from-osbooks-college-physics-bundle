                         </m:mrow>
                         <m:mo/>
                         <m:mi>ω</m:mi>
                         <m:mo/>
                         <m:mi>t</m:mi>
                       </m:mrow>
                     </m:msup>
                     <m:mo/>
                     <m:mrow>
                       <m:mo form="prefix">ⅆ</m:mo>
                       <m:mi>t</m:mi>
                     </m:mrow>
                   </m:mrow>
                 </m:mrow>
               </m:mrow>
             </m:mtd>
