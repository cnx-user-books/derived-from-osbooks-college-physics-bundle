</figure></solution></exercise><exercise id="fs-id1165134378618">
<problem id="fs-id1165134378620">
<para id="fs-id1165134378622"><m:math>
 <m:mrow>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mi>π</m:mi>
   <m:mn>4</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>r</m:mi><m:mtext> </m:mtext><m:mo>≥</m:mo><m:mn>−3</m:mn></m:mrow>
