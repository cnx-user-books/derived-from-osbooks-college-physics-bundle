  </m:mfrac>
  <m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>r</m:mi><m:mtext> </m:mtext><m:mo>≥</m:mo><m:mtext> </m:mtext><m:mn>2</m:mn></m:mrow>
